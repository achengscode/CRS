<GridPane alignment="CENTER" gridLinesVisible="true" hgap="0.0" maxHeight="+Infinity" maxWidth="+Infinity" minWidth="-Infinity" styleClass="title-pane" vgap="0.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <Text fill="WHITE" text="Welcome" GridPane.columnIndex="0" GridPane.columnSpan="1" GridPane.rowIndex="0" />
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="+Infinity" minWidth="600.0" prefWidth="1077.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="20.0" vgrow="NEVER" />
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <TabPane prefHeight="800.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" GridPane.columnIndex="0" GridPane.rowIndex="1">
    <tabs>
      <Tab text="Generate Report">
        <content>
          <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
              <Label layoutX="18.0" layoutY="19.0" text="Select Report Type" />
              <ComboBox layoutX="129.0" layoutY="17.0" prefHeight="21.0" prefWidth="105.0" promptText="select">
                <items>
                  <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Daily Rental" />
                    <String fx:value="Weekly Rental" />
                    <String fx:value="Daily Return" />
                    <String fx:value="Weekly Return" />
                  </FXCollections>
                </items>
              </ComboBox>
            </children>
          </AnchorPane>
        </content>
      </Tab>
      <Tab fx:id="add" text="Add Vehicle">
        <content>
          <AnchorPane id="Content" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="379.0" prefWidth="1077.0">
            <children>
              <Label layoutX="25.0" layoutY="24.0" text="Vehicle ID" />
              <TextField id="VehicleID" fx:id="vehicleID" layoutX="158.0" layoutY="24.0" prefWidth="200.0" />
              <Label layoutX="25.0" layoutY="85.0" text="Vehicle Model" />
              <TextField fx:id="model" layoutX="158.0" layoutY="82.0" prefWidth="200.0" promptText="" />
              <Label layoutX="25.0" layoutY="142.0" text="Vehicle Type" />
              <Label layoutX="418.0" layoutY="24.0" text="Plate Number" />
              <TextField id="Plate" fx:id="plate" layoutX="496.0" layoutY="24.0" prefWidth="200.0" />
              <Label layoutX="418.0" layoutY="91.0" prefWidth="37.0" text="Make" />
              <TextField fx:id="make" layoutX="496.0" layoutY="82.0" prefWidth="200.0" />
              <Button id="addVehicle" layoutX="579.0" layoutY="321.0" mnemonicParsing="false" onAction="#addVehicle" prefWidth="117.0" text="Add Vehicle" />
              <ComboBox fx:id="type" layoutX="158.0" layoutY="133.0" prefHeight="22.0" prefWidth="105.0" promptText="select">
                <items>
                  <FXCollections fx:factory="observableArrayList">
                    <String fx:value="Car" />
                    <String fx:value="Truck" />
                  </FXCollections>
                </items>
              </ComboBox>
              <ComboBox id="type" fx:id="category" layoutX="491.0" layoutY="134.0" prefHeight="21.0" prefWidth="105.0" promptText="select">
                <items>
                  <FXCollections fx:factory="observableArrayList" />
                </items>
              </ComboBox>
              <Label layoutX="418.0" layoutY="139.0" text="Category" />
              <TextField id="make" fx:id="manufacturing" layoutX="158.0" layoutY="195.0" prefWidth="200.0" />
              <Label layoutX="25.0" layoutY="201.0" text="Year of Manufacturing" />
              <TextField id="make" fx:id="color" layoutX="496.0" layoutY="193.0" prefWidth="200.0" />
              <Label layoutX="418.0" layoutY="193.0" text="Color" />
              <Label fx:id="idLabel" layoutX="158.0" layoutY="46.0" text="Vehicle ID must be provided" textFill="RED" visible="false" />
              <Label fx:id="plateLabel" layoutX="496.0" layoutY="49.0" text="Plate Number must be provided" textFill="RED" visible="false" />
              <Label fx:id="modelLabel" layoutX="158.0" layoutY="104.0" text="Vehicle Model must be provided" textFill="RED" visible="false" />
              <Label fx:id="makeLabel" layoutX="496.0" layoutY="107.0" text="Make should be provided" textFill="RED" visible="false" />
              <Label fx:id="typeLabel" layoutX="158.0" layoutY="152.0" text="Please Select a Type" textFill="RED" visible="false" />
              <Label fx:id="categoryLabel" layoutX="491.0" layoutY="160.0" text="Please Select a Category" textFill="RED" visible="false" />
              <Label fx:id="yearLabel" layoutX="154.0" layoutY="226.0" text="Please provide a Year" textFill="RED" visible="false" />
              <Label fx:id="numberLabel" layoutX="154.0" layoutY="229.0" text="Year must be a number" textFill="RED" visible="false" />
              <Label fx:id="colorLabel" layoutX="496.0" layoutY="223.0" text="Please provide a Color" textFill="RED" visible="false" />
              <Label fx:id="idInvalid" layoutX="158.0" layoutY="46.0" text="Invalid Vehicle ID" textFill="RED" visible="false" />
            </children>
          </AnchorPane>
        </content>
      </Tab>
      <Tab text="Vehicle List">
        <content>
          <AnchorPane id="Content" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <children>
              <Button layoutX="317.0" layoutY="185.0" mnemonicParsing="false" text="Sell Vehicle" />
              <Button layoutX="573.0" layoutY="185.0" mnemonicParsing="false" text="Remove Vehicle" />
              <Button layoutX="434.0" layoutY="185.0" mnemonicParsing="false" text="Update Vehicle" />
              <Label layoutX="14.0" layoutY="37.0" text="Vehicle ID" />
              <TextField layoutX="94.0" layoutY="34.0" prefWidth="200.0" />
              <Label layoutX="389.0" layoutY="35.0" text="Vehicle Model" />
              <TextField layoutX="481.0" layoutY="32.0" prefWidth="200.0" />
            </children>
          </AnchorPane>
        </content>
      </Tab>
    </tabs>
  </TabPane>
  <stylesheets>
    <URL value="@style.css" />
  </stylesheets>
</GridPane>
